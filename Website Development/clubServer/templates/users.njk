{% extends "clubBase.njk" %}
{% block title %}
    <title>Club Members</title>
{% endblock %}

{% block main %}
    <h1>Raccoon Lovers Of California</h1>
    <table>
        <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Role</th>
        </tr>
        </thead>
        <tbody>
        {% for user in users %}
            <tr>
                <td>{{ user.firstName }} {{ user.lastName }}</td>
                <td> {{user.email}} </td>
                <td>{{user.role}}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}